
{% extends "index.njk" %} 
{% block content %}
      <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
        <h1 class="h2">{{ title }}</h1>
      </div>
      <div class="table-responsive">
        <table class="table table-striped table-sm">
          <tbody>
          {% for t in tickets %}
            {{ renderTicketRow(t) }}
          {% endfor %}
          </tbody>
        </table>
        <br/><br/><br/><br/><br/>
        <button onclick="onBtnDeleteAllTickets()">Delete All Tickets</button>
        <button onclick="onBtnDeleteAllTicketsAndUsers()">Delete All Tickets And Users</button>
      </div>
{% endblock %}


