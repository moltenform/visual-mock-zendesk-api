{% macro renderTicketRow(objTicket) %}
  <tr>
    <td><a href="/agent/tickets/{{ objTicket.id }}">{{ objTicket.id }}</a></td>
    <td>{{ objTicket.plain_body }}</td>
    <td>{{ objTicket.requester_rendered }}</td>
    <td>{{ objTicket.created_at_rendered }}</td>
    <td>{{ objTicket.status }}</td>
  </tr>
{% endmacro %}

<div class="container-fluid">
  <div class="row">
    <nav id="sidebarMenu" class="col-md-3 col-lg-2 d-md-block bg-light sidebar collapse">
      <div class="sidebar-sticky pt-3">
        <ul class="nav flex-column">
          <li class="nav-item">
            <a class="nav-link" href="/web/tickets-open">
              <span data-feather="file"></span>
              Open Tickets
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/web/tickets-resolved">
              <span data-feather="file"></span>
              Resolved Tickets
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/web/tickets-new">
              <span data-feather="file"></span>
              New Ticket
            </a>
          </li>
        </ul>
      </div>
    </nav>
    <main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-md-4">
      {% block content %}{% endblock %}
    </main>
  </div>
</div>
